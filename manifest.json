{
  "manifest_version": 3,
  "name": "QuickFill",
  "version": "1.0",
  "description": "Instantly fill web forms with realistic fake data",
  
  "action": {
    "default_popup": "popup.html",
    "default_title": "QuickFill - Fill forms instantly"
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  
  "permissions": [
    "activeTab",
    "storage"
  ]
}